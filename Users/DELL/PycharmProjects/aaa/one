first = input("enter first number : ")
operator = input("enter operator  (+,-,*,/,%) : ")
second = input ("enter second number : ")

if operator == "+":
    print(first + second)
    if operator == "-":
    print(first - second)
     if operator == "*":
    print(first * second)
    if operator == "/":
    print(first / second)
    if operator == "%":
    print(first % second)
